  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --open",
    "build-dev": "cross-env NODE_ENV=development webpack",
    "build-prod": "cross-env NODE_ENV=production webpack",
    "format": "prettier --write \"src/**/*.{js,jsx,ccs,scss}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx}\"",
    "package-all": "yarn build && electron-builder build -mwl",
    "package-ci": "yarn postinstall && yarn build && electron-builder --publish always",
    "package-mac": "yarn build && electron-builder build --mac",
    "package-linux": "yarn build && electron-builder build --linux",
    "package-win": "yarn build --nomaps && electron-builder build --win --x64",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "eject": "react-scripts eject",
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "electron-build-win": "electron-builder --win --x64",
    "release": "npm run react-build && electron-builder --publish=always",
    "build-electron": "npm run react-build && npm run electron-build",
    "build-electron-win": "npm run build && npm run electron-build-win",
    "electron-start": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\""
  },
